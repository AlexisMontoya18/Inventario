@model IEnumerable<SystemaVidanta.Models.Bitacora>

@{
    ViewBag.Title = "Index";
}

<h2>Bitacora</h2>


<table class="well table">
    
    
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Accion)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Tabla)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.PropertyModified)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.PrimaryKeyValue)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.OldValue)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.NewValue)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.IdUsuario)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Fecha)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Accion)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Tabla)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.PropertyModified)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.PrimaryKeyValue)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.OldValue)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.NewValue)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IdUsuario)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Fecha)
            </td>
            <td>



                <a href="@Url.Action("Details", new { id = item.Id })" class="btn btn-info btn-xs">Detalle <span class="glyphicon glyphicon-info-sign"></span></a><br />

            </td>
        </tr>
    }

</table>
